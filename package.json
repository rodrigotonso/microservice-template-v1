  "repository": {
    "type": "git",
    "url": "git+URL GIT"
  },
  "keywords": [],
  "bugs": {
    "url": "get+URL",
    "mail": ""
  },
  "scripts": {
    "_clean": "node ./package_utils/cleanBuild.js",
    "_copy-files": "copyfiles -e \"**/*.{js,ts,tsx,jsx}\" -u 1 --soft -V \"src/**/*\" build",
    "_control": "npm run lint && npm run format && npm run test:unit && npm run test:integration",
    "lint": "eslint src -c .eslintrc.json --ext .ts --ext .tsx --ext js --ext .jsx --fix --cache",
    "format": "prettier --write --config .prettierrc src",
    "dev": "npm run _clean && npm run _copy-files && link-module-alias && tsc-watch --onSuccess \"node -r dotenv/config ./build/index.js dotenv_config_path=./.env\" --onFailure \"echo Error!\" --noClear",
    "test:unit": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/unit.html jest --no-cache --forceExit --config jestconfig.json --detectOpenHandles ./tests/unit",
    "test:integration": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/integration.html jest --no-cache --forceExit --config jestconfig.json --runInBand --detectOpenHandles ./tests/integration",
    "compile": "npm run _clean && tsc --project tsconfig.json && npm run _copy-files",
    "build": " npm run _control && npm run compile && npm run generate-docs && npm audit --production  && echo \"Finished!\"",
    "start": "link-module-alias && node -r dotenv/config ./build/index.js dotenv_config_path=./.env",
    "pm2": "link-module-alias && pm2 start pm2.config.js",
    "start-from-docker": "link-module-alias >/dev/null 2>&1 && node ./build/index.js",
    "generate-docs": "ts-node ./package_utils/generateAPIDocs.ts && typedoc --options ./typedoc.json && node ./package_utils/cleanTypedocDocs.js"
  },
  "files": [
    "build/**/*"
  ],
  "_moduleAliases": {
    "~": "build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && npm run test:unit"
    }
  },
  "dependencies": {
    "ajv": "8.11.0",
    "axios": "0.26.1",
    "body-parser": "1.20.0",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-graphql": "0.12.0",
    "express-jwt": "7.4.3",
    "express-rate-limit": "6.4.0",
    "express-slow-down": "1.4.0",
