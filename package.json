  "license": "MIT",
  "homepage": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+URL GIT"
  },
  "keywords": [],
  "bugs": {
    "url": "get+URL",
    "mail": ""
  },
  "scripts": {
    "_clean": "node ./package_utils/cleanBuild.js",
    "_copy-files": "copyfiles -e \"**/*.{js,ts,tsx,jsx}\" -u 1 --soft -V \"src/**/*\" build",
    "_control": "npm run lint && npm run format && npm run test:unit && npm run test:integration",
    "lint": "eslint src -c .eslintrc.json --ext .ts --ext .tsx --ext js --ext .jsx --fix --cache",
    "format": "prettier --write --config .prettierrc src",
    "dev": "npm run _clean && npm run _copy-files && link-module-alias && tsc-watch --onSuccess \"node -r dotenv/config ./build/index.js dotenv_config_path=./.env\" --onFailure \"echo Error!\" --noClear",
    "test:unit": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/unit.html jest --no-cache --forceExit --config jestconfig.json --detectOpenHandles ./tests/unit",
    "test:integration": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/integration.html jest --no-cache --forceExit --config jestconfig.json --runInBand --detectOpenHandles ./tests/integration",
    "compile": "npm run _clean && tsc --project tsconfig.json && npm run _copy-files",
    "build": " npm run _control && npm run compile && npm run generate-docs && npm audit --production  && echo \"Finished!\"",
    "start": "link-module-alias && node -r dotenv/config ./build/index.js dotenv_config_path=./.env",
    "pm2": "link-module-alias && pm2 start pm2.config.js",
    "start-from-docker": "link-module-alias >/dev/null 2>&1 && node ./build/index.js",
    "generate-docs": "ts-node ./package_utils/generateAPIDocs.ts && typedoc --options ./typedoc.json && node ./package_utils/cleanTypedocDocs.js"
  },
  "dependencies": {
    "ajv": "6.12.5",
    "axios": "0.24.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-graphql": "0.12.0",
    "express-jwt": "6.0.0",
    "express-rate-limit": "5.1.3",
    "express-slow-down": "1.3.1",
    "graphql": "15.5.0",
    "handlebars": "4.7.7",
    "helmet": "4.1.1",
    "link-module-alias": "1.2.0",
    "lodash": "4.17.21",
    "mariadb": "2.4.2",
    "moment": "2.29.2",
    "mongoose": "6.0.14",
    "mysql2": "2.2.1",
    "pg": "8.3.3",
    "pg-hstore": "2.3.3",
    "redis-simple-client": "1.0.6",
    "sequelize": "6.9.0",
    "stack-trace": "0.0.10",
    "swagger-ui-express": "4.3.0",
    "urlencode": "1.1.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/express": "4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/express-rate-limit": "5.1.0",
    "@types/express-slow-down": "1.3.0",
    "@types/jest": "26.0.13",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.168",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/redis": "2.8.27",
    "@types/sequelize": "4.28.9",
    "@types/stack-trace": "0.0.29",
    "@types/supertest": "2.0.10",
    "@types/swagger-ui-express": "4.1.2",
    "@types/urlencode": "1.1.2",
    "@types/yamljs": "0.2.31",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "copyfiles": "2.4.0",
    "cross-env": "7.0.2",
    "eslint": "7.21.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.0",
    "husky": "7.0.4",
    "jest": "^26.1.0",
    "jest-html-reporter": "3.2.0",
    "jsonwebtoken": "8.5.1",
    "prettier": "^2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "26.4.0",
    "ts-node": "9.0.0",
    "tsc-watch": "4.2.9",
    "typedoc": "0.22.15",
    "typedoc-plugin-external-module-name": "4.0.3",
    "typescript": "3.9.5"
  },
  "files": [
    "build/**/*"
  ],
  "_moduleAliases": {
    "~": "build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && npm run test:unit"
    }
  }
}
